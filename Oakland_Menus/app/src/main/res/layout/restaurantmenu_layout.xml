<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:title="@string/action_settings"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="#9A844A"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/"
    tools:context=".RestaurantMenuActivity"


    >

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/Chick"
        android:background="@drawable/Chick"

        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:id="@+id/Subway"
        android:background="@drawable/Subway_logo"
        android:contentDescription="@string/Subway"
        android:layout_below="@+id/Chick"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="44dp" />

    <!--<ImageButton
        android:layout_width="100dp"
